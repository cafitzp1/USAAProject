<!DOCTYPE html>
<html lang="en">

<head>
  <%- include partials/head_elements.ejs %>

  <style>
    .bd-placeholder-img {
      font-size: 1.125rem;
      text-anchor: middle;
      -webkit-user-select: none;
      -moz-user-select: none;
      -ms-user-select: none;
      user-select: none;
    }

    @media (min-width: 768px) {
      .bd-placeholder-img-lg {
        font-size: 3.5rem;
      }
    }

    body {
      padding-top: 5rem;
      background-color: rgb(136, 146, 156);
    }

    .card {
      background-color: rgb(195, 202, 204);
    }

    .starter-template {
      padding: 3rem 1.5rem;
      text-align: center;
    }

    .blank {
      height: 2rem;
    }
  </style>
</head>

<body class="has-feed">
  <%- include partials/navigation.ejs %>

  <!-- Page Content -->
  <div class="container">

    <div class="row">

      <!-- Blog Entries Column -->
      <div class="col-md-8">

        <!-- Post and News Feed sections from separate file to reduce redundancy -->
        <%- include partials/post_section_anonymous %>
        <%- include partials/news_feed %>

        <!-- Pagination -->
        <ul class="pagination justify-content-center mb-4">
          <li class="page-item">
            <a class="page-link" href="#">&larr; Older</a>
          </li>
          <li class="page-item disabled">
            <a class="page-link" href="#">Newer &rarr;</a>
          </li>
        </ul>

      </div>

      <%- include partials/sidebar_anonymous   %>

    </div>

    <!-- End Page Content -->
    <%- include partials/script_elements.ejs %>
  </div>
</body>

</html>